<?xml version="1.0" encoding="UTF-8"?>

<form name="form1" title="" fontSize="" scanPicture="">
	<ui-settings mode="page" fontSize="11"></ui-settings><!-- step|page -->
	<print-settings weight="210" height="297" xOffset="0"
